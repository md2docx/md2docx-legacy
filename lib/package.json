{
  "name": "@m2d/md2docx",
  "author": "Mayank Kumar Chaudhari (https://mayank-chaudhari.vercel.app)",
  "private": false,
  "version": "0.0.0",
  "description": "The simplest way to convert Markdown to DOCX. Batteries-included wrapper around all @m2d/* packages and Unified. One import, one call — full Markdown → Word conversion with HTML, images, math, tables, and more.",
  "license": "MPL-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": "github:md2docx/md2docx",
  "bugs": "https://github.com/md2docx/md2docx/issues",
  "homepage": "https://github.com/md2docx/md2docx/#readme",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup && tsc -p tsconfig-build.json && gzip -c dist/index.js | wc -c",
    "clean": "rm -rf dist",
    "dev": "tsup --watch && tsc -p tsconfig-build.json -w",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.6.2",
    "@vitest/coverage-v8": "^3.2.4",
    "docx": "^9.5.1",
    "esbuild-plugin-rdi": "^0.0.0",
    "jsdom": "^27.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@m2d/core": "^1.7.0",
    "@m2d/emoji": "^0.1.3",
    "@m2d/html": "^1.1.10",
    "@m2d/image": "^1.4.0",
    "@m2d/list": "^0.0.8",
    "@m2d/math": "^0.0.6",
    "@m2d/mermaid": "^1.2.2",
    "@m2d/table": "^0.1.1",
    "docx": "^9.3.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/md2docx"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/mayank1513"
    }
  ],
  "keywords": [
    "markdown to docx",
    "md to docx",
    "markdown docx converter",
    "docx generator",
